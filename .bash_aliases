# some more ls aliases
alias ll='ls -l'
alias la='ls -A'
alias l='ls -CF'
alias ..="cd .."
alias ...="cd ../.."
alias cgrep='find ./ -name "*.c" | xargs grep'
alias hgrep='find ./ -name "*.h" | xargs grep'
alias pygrep='find ./ -name "*.py" | xargs grep'
alias jsgrep='find ./ -name "*.js" | xargs grep'
alias grep='ack'


